#include<stdio.h>
void main()
{
int n,k,i,sum_even=0,sum_odd=0;
printf("Enter an integer n:");
scanf("%d",&n);
printf("Enter an integer k:");
scanf("%d",&k);
i=1;
while(i<=n)
{
if(i%k==0)
{
printf("\n%d is divisible by %d",i,k);
if(i%2==0)
sum_even+=i;
else
sum_odd+=i;
}
i++;
}
printf("\nsum of even numbers divisible by %d",k,sum_even);
printf("\nsum of odd numbers divisible by %d",k,sum_odd);
getch();
}
